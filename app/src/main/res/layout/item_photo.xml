    android:layout_height="wrap_content">


    <ImageView
        android:id="@+id/iv_get_image"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginTop="15dp"
        android:src="@drawable/sublimt" />

    <ImageView
        android:id="@+id/iv_del_icon"
